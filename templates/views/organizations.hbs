{{>header}}

    <div class="content">
        <div class="row">
            <div class="col-md-12">
                <div class="card">
                    <div class="card-header">
                        <h4 class="card-title"> Organization List</h4>
                    </div>
                    <div class="card-body">
                        <div class="table-responsive">
                            <table class="table">
                                <thead class=" text-primary">
                                    <th>
                                        ID      
                                    </th>
                                    <th>
                                        Name
                                    </th>
                                    <th>
                                        Status
                                    </th>
                                    <th class="text-right">
                                        Date Created
                                    </th>
                                </thead>
                                <tbody>
                                    {{#if data}}
                                     {{#each data}}
                                    <tr>
                                        <td>{{this.id}}</td>
                                        <td>{{this.name}}</td>
                                        <td>{{this.status}}</td>
                                         <td class="text-right">{{this.create_date}}</td>
                                    </tr>
                                     {{/each}}
                                    {{/if}}

                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
{{>footer}}


</body>

</html>